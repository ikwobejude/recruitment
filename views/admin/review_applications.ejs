<!DOCTYPE html>
<html lang="en">

<head>
<%- include('../layouts/admin/header') %>
<head>

    <%- include('../layouts/admin/side-bar') %>

        <main class="main-content position-relative max-height-vh-100 h-100 border-radius-lg ">
            <!-- Navbar -->
            <%- include('../layouts/admin/nav') %>
                <!-- End Navbar -->
                <div class="container-fluid py-4">


                    <div class="row">
                        <div class="col-md-12 mt-4">
                            <div class="card">
                                <div class="card-header pb-0 px-3">
                                    <div style="text-align: center">
                                        <h6 class="mb-0"><%= count %> <%= orgid == 8 ? "COMPLETED APPLICATIONS" : "UNCOMPLETED APPLICATIONS"   %></h6>
                                    </div>
                                    
                                </div>
                                <div class="card-body ">
                                    <div class="table-responsive">
                                        <table class="table table-bordered   mb-0">
                                            <thead>
                                              <tr>
                                                <th class="text-uppercase  text-sm">.</th>
                                                <th class="text-uppercase  text-sm">NAME</th>
                                                <th class="text-uppercase  text-sm">MDA</th>
                                                <th class="text-uppercase  text-sm">POSITION</th>
                                                
                                                <th class="text-uppercase  text-sm">HIGHEST QUALI.</th>
                                                <th class="text-uppercase  text-sm">location</th>
                                                
                                               
                                              </tr>
                                            </thead>
                                            <tbody>
                                                <% applicant.forEach(function(res){%>
                                              <tr>
                                                <td>
                                                    <a class="btn btn-link text-dark px-3 mb-0" href="/admin/application?process_id=<%= res.process_number %>"><i
                                                        class="fas fa-eye text-dark me-2"
                                                        aria-hidden="true"></i>View</a>
                                                  </td>
                                                <td>
                                                  <div class="d-flex  py-1">
                                                    <!-- <div>
                                                      <img src="../assets/img/small-logos/logo-xd.svg" class="avatar avatar-sm me-3" alt="xd">
                                                    </div> -->
                                                    <div class="d-flex ">
                                                      <h6 class="text-sm font-weight-bold"><%=  res.applicant_fullname.length > 20 ?  res.applicant_fullname.slice(0, 20)+'...' :  res.applicant_fullname %> <br> </h6>
                                                    </div>
                                                  </div>
                                                </td>
                                                <td>  <span class="text-sm font-weight-bold"><%= res. name %></span>  </td>
                                                <td>  <span class="text-sm font-weight-bold"><%= res. position %></span>  </td>
                                               
                                                <td class="align-middle">
                                                  <span class="text-sm font-weight-bold"><%= res.highest_education. length > 20 ? res.highest_education.slice(0, 20)+'...' : res.highest_education %> %></span>
                                               </td>
                                               <td class="align-middle">
                                                <span class="text-sm font-weight-bold"><%= res.gender %>.<%= res.marital_status %></span>
                                              </td>
                                             
                                              </tr>
                                              <%});%>
                                              
                                            </tbody>
                                          </table>
                                    </div>
                                   
                                    

                                    <div class="card-body p-3">
                                         <% let uri = orgid == 8? '/admin/admin_view_applications?complect=8&': '/admin/admin_view_applications?' %>
                                        <% if (pages > 0) { %>
                                       


                                            <ul class="pagination pagination-primary">
                                                <% if (current == 1) { %>
                                                    <li class="page-item disabled"><a class="page-link" href="javascript:void(0)" tabindex="-1">First</a></li>
                                                <% } else { %>
                                                    <li class="page-item "><a class="page-link" href="<%= uri%>" tabindex="-1"><<</a></li>
                                                <% } %>
                                                <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
                                                <% if (i !== 1) { %>
                                                    <li class="page-item"><a class="page-link" href="javascript:void(0)">...</a></li>
                                                <% } %>
                                                <% for (; i <= (Number(current) + 4) && i <= pages; i++) { %>
                                                    <% if (i == current) { %>
                                                    <li class="page-item active"><a class="page-link" href="javascript:void(0)"><%= i %> </a></li>
                                                    <% } else { %>
                                                    <li class="page-item"><a class="page-link"  href="<%= uri%>page=<%= i %>"><%= i %></a></li>
                                                    <% } %>
                                                    <% if (i == Number(current) + 4 && i < pages) { %>
                                                        <li class="page-item"><a class="page-link"  href="javascript:void(0)">...</a></li>
                                                    <% } %>
                                                <% } %>
                                                <% if (current == pages) { %>
                                                <li class="page-item"><a class="page-link" href="javascript:void(0)">>></a></li>
                                                <% } else { %>
                                                    <li class="page-item"><a class="page-link" href="<%= uri%>page=<%= pages %>">Last</a></li>
                                                <% } %>
                                            </ul>
                                        <% } %>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>




                    <%- include('../layouts/admin/footer') %>
                        </body>

</html>